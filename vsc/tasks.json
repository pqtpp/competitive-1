{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "c++ build for AtCoder",
            "type": "shell",
            "options": {
                "cwd": "${fileDirname}"
            },
            "command": "g++",
            "args": [
                "-D",
                "poe",
                "-g",
                "-O2",
                "${file}",
                "-I",
                "/workspaces/competitive/library",
                "-o",
                "/workspaces/competitive/a.out"
            ],
            "group": "build"
        }
    ]
}